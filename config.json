{
  "inputs": {
    "qlab": {
      "className": "qlab",
      "settings": {
        "#listenIP": "IP to listen on for OSC responses",
        "listenIP": "0.0.0.0",
        "port": 53001
      }
    },
    "GPIObuttons": {
      "className": "GPIObuttons",
      "settings": {
        "#debounceTime": "Time to wait for change of state",
        "debounceTime": 0.2,
        "#repeatTime": "Time before other switch can be used",
        "repeatTime": 0.05
      }
    }
  },
  "outputs": {
    "qlab1": {
      "className": "qlab",
      "settings": {
        "ip": "192.168.5.106",
        "port": 53000
      }
    },
    "qlab2": {
      "className": "qlab",
      "settings": {
        "ip": "192.168.5.108",
        "port": 53000
      }
    }
  },
  "actions": [
    {
      "inputName": "qlab",
      "oscExpr": "/reply/cue_id/*/displayName",
      "output": {
        "comment": "displayStuff"
      }
    },
    {
      "inputName": "GPIObuttons",
      "GPIO": 18,
      "description": "Go",
      "output": {
        "outputName": "qlab1",
        "message": "/go"
      }
    },
    {
      "inputName": "GPIObuttons",
      "GPIO": 27,
      "description": "Panic",
      "output": {
        "outputName": "qlab1",
        "message": "/panic"
      }
    },
    {
      "inputName": "GPIObuttons",
      "GPIO": 22,
      "description": "Pause",
      "output": {
        "outputName": "qlab1",
        "message": "/pause"
      }
    },
    {
      "inputName": "GPIObuttons",
      "GPIO": 23,
      "description": "Go",
      "output": {
        "comment": "poweroff"
      }
    }
  ]
}
