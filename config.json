{
  "settings": {
    "#debounceTime": "Time before other switch can be used",
    "debounceTime": 0.2,
    "#listenIP": "IP to listen on for OSC responses",
    "listenIP": "0.0.0.0"
  },
  "oscServers": {
    "mac": {
      "ip": "192.168.5.106",
      "port": 53000,
      "responsePort": 53001,
      "responseCallback": {
        "/reply/cue_id/*/displayName": "qlab.display_handler"
      }
    }
  },
  "importModules": [
    "qlab"
  ],
  "keyActions": {
    "charLCD": [
      {
        "keyCode": 0,
        "description": "Stop",
        "lcdMessage": "Stop",
        "lcdColor": [ 1,1,1 ],
        "OSC": [
          {"mac": "/stop"}
        ],
        "Actions": [
          "qlab.displayNext"
        ]
      },
      {
        "keyCode": 1,
        "description": "Play",
        "lcdMessage": "Play",
        "lcdColor": [ 0,1,0 ],
        "OSC": [
          {"mac": "/go"}
        ],
        "Actions": [
          "qlab.displayCurrentNext"
        ]
      },
      {
        "keyCode": 2,
        "description": "Prev",
        "lcdMessage": "Prev",
        "lcdColor": [ 1,1,1 ],
        "OSC": [
          {"mac": "/playhead/previous"}
        ],
        "Actions": [
          "qlab.displayCurrentNext"
        ]
      },
      {
        "keyCode": 3,
        "description": "Fwd",
        "lcdMessage": "Fwd",
        "lcdColor": [ 1,1,1 ],
        "OSC": [
          {"mac": "/playhead/next"}
        ],
        "Actions": [
          "qlab.displayCurrentNext"
        ]
      },
      {
        "keyCode": 4,
        "description": "Pause",
        "lcdMessage": "Pause",
        "lcdColor": [ 0,1,1 ],
        "OSC": [
          {"mac": "/pause"}
        ],
        "Actions": [
          "qlab.displayCurrentNext"
        ]
      }
    ]
  }
}
